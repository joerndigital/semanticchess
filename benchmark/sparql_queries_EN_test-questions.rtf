\f0\fs32 \cf2 \expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec2 Who has won the DSB Kongress most often?
\f2\fs22 \cf0 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 \
\pard\pardeftab720\partightenfactor0
\cf0 SELECT DISTINCT ?player ?event ((count(?win) as ?wins) + ((xsd:double(COUNT(?draw))/2)) AS ?points) \
WHERE \{\
	\{?game prop:black ?player .\
	 ?game prop:result '0-1' .\
	 ?game prop:result ?win .\
	\}\
	UNION\
	\{?game prop:white ?player .\
	 ?game prop:result '1-0' .\
	 ?game prop:result ?win .\
	\}\
	UNION\
	\{?game prop:white|prop:black ?player .\
	 ?game prop:result '1/2-1/2' .\
	 ?game prop:result ?draw .\
	\}\
        ?game prop:event ?event .\
        FILTER regex(?event, 'DSB Kongress', 'i')\
\
\} ORDER BY ASC(?event) DESC(?points)\
\
\pard\pardeftab720\sl380\partightenfactor0

